[run]
  source = cfdtools
  omit=*/__init__.py
    *.nogit/*.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    if self.debug:
    error_stop
    if settings.DEBUG
    raise AssertionError
    raise NameError
    raise NotImplementedError
    if 0:
    if __name__ == __main__:
    import doctest
    doctest.testmod()